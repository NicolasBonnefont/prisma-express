generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Usuario {
  id      Int    @id @default(autoincrement())
  usuario String @unique
  nome    String
  email   String
  senha   String

  unidades Unidades[]
}

model Unidades {
  id   Int    @id @default(autoincrement())
  nome String

  Usuario    Usuario? @relation(fields: [usuario_id], references: [id])
  usuario_id Int?
}
